<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Aether Bio</title>
  <link href="https://fonts.googleapis.com/css2?family=Overpass:wght@300;400;600;800&family=Rajdhani:wght@700&display=swap" rel="stylesheet">
  <link rel="stylesheet" href="styles/main.css">
</head>
<body id="home">
  <div class="site-container">
    <!-- @import "./includes/header.kit" -->
    <main class="site-main">
      <section class="page-intro-section home-intro-section">
        <div class="page-intro-image-container">
          <img src="assets/images/intro-image-placeholder.gif" alt="Home Intro Image" class="page-intro-image">
        </div>
        <div class="page-intro-content">
          <div class="content-section-inner home-title-inner">
            <h2 class="page-intro-title">
              <span class="page-intro-line page-intro-line-one"></span>
              <span class="page-intro-line page-intro-line-two"></span>
            </h2>
          </div>
        </div>
      </section>
      <section class="content-section home-quote-section full-bleed">
        <div class="content-section-inner home-quote-inner">
          <blockquote class="home-quote">
            
          </blockquote>
        </div>
      </section>
      <section class="content-section home-video-section">
        <div class="content-section-inner">
          <h3 class="content-section-title home-video-title"></h3>
          <div class="video-container home-video-container">
            <iframe class="video-iframe" src="" frameborder="0" allow="autoplay; fullscreen" allowfullscreen></iframe>
          </div>
          <div class="content-section-text-container home-video-text-container"></div>
        </div>
      </section>
      <section class="content-section white home-people-section">
        <div class="content-section-inner">
          <div class="content-section-title-text">
            <h3 class="content-section-title blue">
              <span class="people-section-line1"></span><br/>
              <span class="people-section-line2"></span>
            </h3>
            <div class="content-section-text home-people-text"></div>
          </div>
          <div class="image-container home-people-image-container">
            <img src="assets/images/intro-image-placeholder.gif" alt="The Aether Team" class="home-people-image">
          </div>
        </div>
      </section>
      <section class="content-section about-values-section">
        <div class="content-section-inner about-values-section-inner">
          <h3 class="content-section-title about-values-title"></h3>
          <div class="about-values-container">
            <ul class="about-values-list">
              
            </ul>
          </div>
        </div>
      </section>
    </main>
    <!-- @import "./includes/footer.kit" -->
  </div>
  <script src="scripts/lib/contentful.browser.min.js"></script>
  <script src="https://player.vimeo.com/api/player.js"></script>
  <script src="scripts/main-min.js"></script>
</body>
</html>